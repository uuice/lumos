{
  "theme": "default",
  "cache": {
    "staticAssets": {
      "enabled": false,
      "maxAge": 31536000
    }
  },
  "plugins": {
    "example-plugin": {
      "enabled": true,
      "options": {
        "customOption": "value"
      }
    },
    "word-count-plugin": {
      "enabled": true,
      "options": {}
    },
    "full-featured-plugin": {
      "enabled": true,
      "options": {}
    },
    "test-all-hooks-plugin": {
      "enabled": true,
      "options": {}
    },
    "md-path-transform-plugin": {
      "enabled": true,
      "options": {}
    },
    "middleware-example-plugin": {
      "enabled": true,
      "options": {}
    }
  },
  "cors": {
    "enabled": true,
    "options": {
      "Access-Control-Allow-Origin": "*",
      "Access-Control-Allow-Methods": "GET,HEAD,PUT,PATCH,POST,DELETE",
      "Access-Control-Allow-Headers": "Content-Type,Authorization,Accept",
      "Access-Control-Allow-Credentials": "true",
      "Access-Control-Max-Age": 86400
    }
  },
  "middleware": {
    "ipWhitelist": [],
    "ipBlacklist": ["192.168.1.100", "10.0.0.5"]
  },
  "nestjs": {
    "enabled": false,
    "port": 3070,
    "mountPath": "/api"
  }
}
